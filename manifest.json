{
  "manifest_version": 2,

  "name": "Goal Crusher",
  "description": "Set your Daily, Weekly, and Yearly Goals",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Crush your Goals"
  },

  "permissions": [
    "activeTab",
    "storage"
  ],

  "chrome_url_overrides" : {
    "newtab": "index.html"
  }
}